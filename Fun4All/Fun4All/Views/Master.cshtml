<body>
  <a name="top"></a>
  @Html.Partial( "Navigation/TopMenu" )


  @RenderBody()


  <div class="container">
    <hr>

   
  </div>
  <footer class="footer-distributed">

			<div class="footer-left">

				<h3>All4Fun</h3>

				<p class="footer-links">
					<a href="#">Om os</a>
					<a href="#">Kontakt</a>
					<a href="#">Ferielukning</a>
					<div class="footer-icons">
					<a href="#"><i class="fa fa-facebook-square" style="font-size:40px;margin:-2px;"></i></a>
			    	</div>
				</p>

				<p class="footer-company-name">All4Fun &copy; 2018</p>
			</div>

			<div class="footer-center">

                <h3>Kunde Service</h3>
                
            	<p class="footer-links">
			        <a href="#">Betaling og Sikkerhed</a>
					<a href="#">Levering og Fragt</a>
					<a href="#">Returnering</a>
					<a href="#">Handelsbetingelser</a>
					<a href="#">Cookie- og Persondatapolitik</a>
				</p>
            </div>
			<div class="footer-right">

                <h3>Tilmeld Nyhedsbrev</h3>

				<form action="/action_page.php">
                  <p class="footer-company-about">
					Tilmeld dig her og få gode tilbud,
					rabatkoder, nyheder og meget mere.
					Få en 20% rabatkode ved tilmelding.
				</p>
                
                  <div class="newsContainer">
                    <input type="text" placeholder="Email address" name="mail" required>
                  </div>
                
                  <div class="newsContainer">
                    <input type="submit" value="Tilmeld" name="submit">
                  </div>
                </form>

			</div>
       
		</footer>
		 <div class="returnTop">
            <div class="paymentOptions">
                <div class="paymentImg"><img src="/media/1044/640px-mastercard_logo.png" height="36px" width="36px"></img></div>
                <div class="paymentImg"><img src="/media/1045/dk_logo_konturstreg.png" height="36px" width="36px"></img></div>
                <div class="paymentImg"><img src=" /media/1046/visa_logo.png" height="36px" width="36px"></img></div>
            </div>
            <a id="back" href="#top">^ Tilbage til toppen</a>
        </div>
